{
    "env": {
        "commonIncludePaths": [
            "${workspaceFolder}/**",
            "${workspaceFolder}/framework/include",
            "${workspaceFolder}/examples/include",
            "${workspaceFolder}/test/unity",
            "${workspaceFolder}/test/timer"
        ],
        "commonDefs": [
            "SAF_USE_INTEL_MKL",
            "SAF_ENABLE_SOFA_READER_MODULE",
            "SAF_ENABLE_TRACKER_MODULE"
        ]
    },
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${commonIncludePaths}",
                "${workspaceFolder}/dependencies/MacOSX/include",
                "/opt/intel/compilers_and_libraries/mac/mkl/include"
            ],
            "defines": [
                "${commonDefs}"
            ],
            "macFrameworkPath": [],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Win64",
            "includePath": [
                "${commonIncludePaths}",
                "${workspaceFolder}/dependencies/Win64/include",
                "C:/Program Files (x86)/IntelSWTools/compilers_and_libraries/windows/mkl/include"
            ],
            "defines": [
                "${commonDefs}"
            ]
        }
    ],
    "version": 4
}